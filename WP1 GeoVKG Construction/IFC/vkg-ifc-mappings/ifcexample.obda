[MappingDeclaration] @collection [[
mappingId	Site
target		ifcowl:site/{id} a ifcowl:IfcSite ; ifcowl:refElevation_IfcSite ifcowl:measure/{id} .
source		SELECT * FROM public."IfcSite"

mappingId	Site_Elevation
target		ifcowl:measure/{id} a ifcowl:IfcLengthMeasure ; <https://w3id.org/express#hasDouble> {RefElevation}^^xsd:double .
source		SELECT * FROM public."IfcSite"
]]
